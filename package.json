{
  "name": "tcc_backend",
  "version": "1.0.0",
  "description": "Sistema de controle de produção e estoque de sucos com acompanhamento de custos e vendas, voltado para pequenas agroindústrias.",
  "main": "index.js",
  "scripts": {
    "dev": "nodemon",
    "build": "tsc",
    "start": "node dist/server.js",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "prisma:validate": "prisma validate --schema=prisma/schema.prisma",
    "prisma:migrate": "prisma migrate dev --name init --schema=prisma/schema.prisma",
    "prisma:generate": "prisma generate --schema=prisma/schema.prisma",
    "prisma:seed": "ts-node -r tsconfig-paths/register prisma/seed.ts",
    "prisma:reset": "prisma migrate reset --schema=prisma/schema.prisma",
    "prisma:format": "prisma format --schema=prisma/schema.prisma",
    "test": "npm run prisma:seed && npx playwright test --workers=1",
    "prisma:migrate:deploy": "prisma migrate deploy --schema=prisma/schema.prisma",
    "postinstall": "prisma generate"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/GustavoHenriqueReblin/tcc_backend.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/GustavoHenriqueReblin/tcc_backend/issues"
  },
  "homepage": "https://github.com/GustavoHenriqueReblin/tcc_backend#readme",
  "dependencies": {
    "axios": "^1.12.2",
    "bcrypt": "^6.0.0",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "jsonwebtoken": "^9.0.2",
    "mariadb": "^3.4.5",
    "node-cron": "^4.2.1",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@playwright/test": "^1.56.1",
    "@prisma/client": "^6.18.0",
    "@types/bcrypt": "^6.0.0",
    "@types/cookie-parser": "^1.4.10",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^24.9.1",
    "@typescript-eslint/eslint-plugin": "^8.46.2",
    "@typescript-eslint/parser": "^8.46.2",
    "eslint": "^9.38.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "globals": "^16.4.0",
    "nodemon": "^3.1.10",
    "prettier": "^3.6.2",
    "prisma": "^6.18.0",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.9.3"
  }
}
